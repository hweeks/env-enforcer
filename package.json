{
  "name": "env-enforcer",
  "version": "1.0.0",
  "description": "an express middleware to enforce env config for health routes",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "git@github.com:hweeks/env-enforcer.git",
  "author": "hammy-late-nite <git@hweeks.com>",
  "license": "WTFPL",
  "scripts": {
    "lint": "eslint --ext .ts,.js ./lib",
    "test": "jest",
    "test:debug": "node debug.js",
    "test:debug:nix": "node --inspect-brk ./node_modules/.bin/jest --runInBand --no-cache --watchAll",
    "test:debug:win": "node --inspect-brk ./node_modules/jest-cli/bin/jest.js --runInBand --no-cache --watchAll",
    "dev": "nodemon -e ts --exec yarn bb",
    "dev:brk": "nodemon -e ts --exec yarn bbb",
    "bbb": "babel-node --inspect-brk --extensions \".js,.ts\"",
    "bb": "babel-node --inspect --extensions \".js,.ts\"",
    "prepublishOnly": "tsc",
    "commit": "cz"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.17",
    "@babel/core": "^7.12.17",
    "@babel/node": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^26.6.3",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "semantic-release": "^17.3.9",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "express": "^4.17.1"
  }
}
